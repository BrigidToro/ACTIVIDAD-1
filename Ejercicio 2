1. INICIO
2. SUMA = 0                         # SUMA = 0
3. X = 20                           # SUMA = 0, X = 20
4. SUMA = SUMA + X                    # SUMA = 0 + 20 = 20, X = 20
5. Y = 40                          # SUMA = 20, X = 20, Y = 40
6. X = X + Y **2                    # SUMA = 20, X = 20 + 40**2 = 1620, Y = 40
7. SUMA= SUMA + X/Y                  # SUMA = 20 + 1620/40 = 61, X = 1620, Y = 40
8. ESCRIBA: "EL VALOR DE LA SUMA ES:", SUMA  # Muestra "EL VALOR DE LA SUMA ES: 61"
9. FIN_INICIO
